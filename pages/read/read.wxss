/* pages/photo/photo.wxss */
.new-list {margin:5px 0 40px 0;position: relative;}
.new-list:after{content:'';width:20px;height:2px;position: absolute;left: 50%;bottom:-20px;margin-left: -10px;background: #333;}
.top {margin:0 10px;}
.website {font-size: 14px;color:#b5b5b6;}
.time {font-size: 14px;color:#b5b5b6;}
.title {font-size: 16px;margin:3px 10px 10px;height: 44px;overflow: hidden;}
.new-pic {width:100%;height: 220px;display: none;}
.pic-on {display: block;}
.video {width:100%;height:220px;}
.video-off {display: none;} 